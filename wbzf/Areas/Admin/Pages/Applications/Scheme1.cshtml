@section Scripts {
    <script>
        $('document').ready(function () {
            showScholarDetails();
        });

        function hideScholarDetails() {
            document.getElementById('pastScholarship').style.display = 'none';
        }

        function showScholarDetails() {
            hideScholarDetails();
            var radios = document.getElementsByName('radioScholar');

            var value;
            for (var i = 0; i < radios.length; i++) {

                if (radios[i].checked) {
                    value = radios[i].value;
                    break;
                }
            }
            if (value == 1) {
                document.getElementById('pastScholarship').style.display = 'block';
            }
        }

    </script>
    <script>
        $('document').ready(function () {
            showDetails();
            $('#inputField').on('input', function () {
                $('#r1name').text($(this).val());
            });
        });

        function hideAllDetails() {
            document.getElementById('father_details').style.display = 'none';
            document.getElementById('mother_details').style.display = 'none';
            document.getElementById('guardian_details').style.display = 'none';
            document.getElementById('pastScholarship').style.display = 'none';
        }

        function showDetails() {
            hideAllDetails();
            var radios = document.getElementsByName('radioInline');

            var value;
            for (var i = 0; i < radios.length; i++) {

                if (radios[i].checked) {
                    value = radios[i].value;
                    break;
                }
            }
            if (value == '@SD.Father') {
                document.getElementById('father_details').style.display = 'block';
            } else if (value == '@SD.Mother') {
                document.getElementById('mother_details').style.display = 'block';
            } else if (value == '@SD.Guardian') {
                document.getElementById('guardian_details').style.display = 'block';
            }
            else if (value == 1) {
                document.getElementById('pastScholarship').style.display = 'block';
            }
        }

    </script>

    <script src="~/js/Image_Upload.js" asp-append-version="true"></script>
